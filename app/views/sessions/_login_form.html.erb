<%= form_for :user, url: "/logged", method: "post" do |f|%>

  <%= flash[:notice] if flash[:notice] %>

  <%= f.label :name, 'User'%>
  <%= f.text_field :name%>
  <%= f.label :password%>
  <%= f.password_field :password%>
  <%= f.submit "Log in"%>

  <%= link_to "New user", "users/new" %>

<% end%>
